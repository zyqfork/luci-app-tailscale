{
	"luci-app-tailscale": {
		"description": "Grant access to Tailscale configuration (Lua version)",
		"read": {
			"file": {
				"/sbin/ip -s -j addr": [ "exec" ],
				"/sbin/logread -e tailscale": [ "exec" ],
				"/usr/sbin/tailscale status --json": [ "exec" ],
				"/usr/sbin/tailscale login": [ "exec" ],
				"/usr/sbin/tailscale logout": [ "exec" ],
				"/usr/sbin/tailscale": [ "exec" ]
			},
			"ubus": {
				"service": [ "list" ]
			},
			"uci": [ "tailscale" ],
			"cgi-io": [ "exec" ]
		},
		"write": {
			"uci": [ "tailscale" ]
		}
	}
}
